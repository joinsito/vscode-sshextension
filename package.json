{
  "name": "sshextension",
  "displayName": "SSHExtension",
  "description": "Extension for SSH connections using the sftp settings",
  "version": "0.1.1",
  "publisher": "joinsito",
  "license": "MIT",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ssh",
    "remote"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#DDCC99",
    "theme": "light"
  },
  "activationEvents": [
    "onCommand:sshextension.openConnection",
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "sshextension.openConnection",
        "title": "Open SSH Connection"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "extensionDependencies": [
    "liximomo.sftp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VitalyKondratiev/SSHExtension.git"
  },
  "bugs": {
    "url": "https://github.com/VitalyKondratiev/SSHExtension/issues"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "eslint": "^4.6.1",
    "@types/node": "^7.0.0",
    "@types/mocha": "^2.2.42",
    "command-exists": "^1.2.2",
    "easy-loop": "^1.7.2",
    "filesize": "^3.5.10",
    "fs-extra": "^4.0.2"
  },
  "dependencies": {
    "command-exists": "^1.2.2",
    "easy-loop": "^1.7.2",
    "filesize": "^3.5.10",
    "fs-extra": "^4.0.2",
    "is-path-inside": "^1.0.0",
    "upath": "^1.0.0"
  }
}
